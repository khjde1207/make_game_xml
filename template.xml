<gameList>
    {{range $val := .}}<game>
        <path>./{{.FilePath}}</path>
        <name>{{.Name}}</name>
        <image>./downloaded_images/{{.ImagePath}}</image>
        <video>./downloaded_videos/{{.VideoPath}}</video> 
        <rating>0</rating>
        <releasedate>{{.CreateTime}}</releasedate>
        <playcount>0</playcount>
        <lastplayed>{{.CreateTime}}</lastplayed>
    </game>
    {{end}}
</gameList> 
